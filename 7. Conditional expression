using System;

namespace App
{
	class Program
	{
		static void Main(string[] args)
		{
			// Comparison operation
			// Logical operations
			int a = 10;
			int b = 4;
			bool c = a == b; // false

			bool d = a != b; // true

			// < > <= >=
			bool f = a > b; // true

			bool e = a < b; // false

			bool g = a >= b; // true

			bool h = a <= 10; // true

			// | || & && ! ^
			bool x1 = (5 > 6) | (4 < 6); // false | true => true
			bool x2 = (5 > 6) || (4 > 6); // false | false => false

			bool x3 = (5 > 6) & (4 < 6); // false | true => false
			bool x4 = (5 > 6) && (4 > 6); // false | false => false
			bool x5 = (5 < 6) && (4 < 6); // true | true => true

			bool t = false;
			bool s = !t; //true

			bool x31 = (5 > 6) ^ (4 < 6); // false | true => true
			bool x41 = (4 < 6) ^ (4 < 6); // false | false => false

			Console.ReadKey();
		}
	}
}
