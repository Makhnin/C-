using System;

namespace App
{
	class Program
	{
		static void Main(string[] args)
		{
			// 1 Increment and decrement
			// 2 Multiplication and division
			// 3 Addition and subtraction

			int a = 3;
			int b = 5;
			int c = 40;
			int d = (c-(--b)) * a;
			// (c--) = 40 | c=39
			// b * a = 15
			// 40 - 15 = 25
			Console.WriteLine($"a={a} b={b} c={c} d={d}");

			Console.ReadKey();
		}
	}
}
