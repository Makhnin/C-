using System;

namespace App
{
	class Program
	{
		static void Main(string[] args)
		{
			var index = 5; // int

			var apple = "Apple"; // string

			object obj = 2;

			object obj2 = "hello world!";

			char a = 'A';

			string hello = "Hello";

			decimal rate = 5.4M;

			double weight = 180.34;

			float height = 166.23F;

			byte b = 3;

			bool isAlive = true;
			isAlive = false;
			int tax = 9;
			tax = 5;
			Console.WriteLine(tax);
			tax = 7;
			Console.ReadKey();
		}
	}
}
